{
  "name": "server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:integration": "jest --testPathPattern=integration --detectOpenHandles",
    "test:unit": "jest --testPathPattern=unit --detectOpenHandles",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "populate-taxonomy": "node scripts/populateTaxonomy.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "migrate:product-approval": "node scripts/addProductApprovalFields.js",
    "migrate:product-approval:rollback": "node scripts/addProductApprovalFields.js --rollback",
    "migration:check": "node scripts/runMigrations.js",
    "feature-flags:status": "node -e \"require('./utils/featureFlags').featureFlags.getHealthStatus(); console.log('âœ… Feature flags loaded successfully')\""
  },
  "author": "eugene",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^4.1.1",
    "nodemailer": "^7.0.3",
    "nodemon": "^3.1.9",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "paypal-rest-sdk": "^1.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-node": "^4.1.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.3",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "controllers/**/*.js",
      "services/**/*.js",
      "models/**/*.js",
      "!node_modules/**"
    ]
  }
}
